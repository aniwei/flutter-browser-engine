{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "target": "ES2021",
    "lib": ["dom", "ES2021"],
    "types": ["@types/jest", "node"],
    "composite": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noImplicitAny": false,
    "noImplicitThis": true,
    "noEmit": false,
    "build": true,
    "typeRoots": [".", "./node_modules/@types"],
    "baseUrl": "./",
    "rootDir": "./",
    "outDir": "dist",
  }
}
